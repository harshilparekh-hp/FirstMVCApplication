
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    
</head>
<body>
    @{ 
        var employee = ViewData["EmployeeData"] as BlankApplication.Models.Employee;
    }

    <div> 
        This is the home page of my MVC application

        <h2>@ViewData["Header"]</h2>
        <table>
            <tr>
                <td>Employee ID: </td>
                <td>@employee.EmployeeId</td>
            </tr>
            <tr>
                <td>Employee Name: </td>
                <td>@employee.Name</td>
            </tr>
            <tr>
                <td>Address: </td>
                <td>@employee.Address</td>
            </tr>
            <tr>
                <td>Gender:  </td>
                <td>@employee.Gender</td>
            </tr>
            <tr>
                <td>City: </td>
                <td>@employee.City</td>
            </tr>
        </table>
    </div>
</body>
</html>
